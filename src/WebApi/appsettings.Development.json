{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://host.docker.internal:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "Bbc"
    }
  },
  "Redis": {
    "ConnectionString": "host.docker.internal:6379"
  },
  "PostgreSQL": {
    "ConnectionString": "Host=host.docker.internal;Port=5432;Database=test;Username=postgres;Password=postgres"
  },
  "Yandex": {
    "Suggest": {
      "ApiKey": "d11a8e1f-7bcf-4671-b093-cbdbcadb7e53",
      "IsDebug": true
    },
    "Geocode": {
      "ApiKey": "1c49d25e-ecad-404c-a6c7-2d8dc5f429e0",
      "IsDebug": true
    },
    "Route": {
      "ApiKey": "not-ready",
      "IsDebug": true
    }
  }
}